<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <defaultDocument>
      <files>
        <clear />
        <add value="index.html" />
      </files>
    </defaultDocument>
    <rewrite>
      <rules>
        <rule name="API Proxy Route" stopProcessing="true">
          <match url="^api/ncsc-rss$" />
          <action type="Rewrite" url="api/ncsc-rss.php" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
